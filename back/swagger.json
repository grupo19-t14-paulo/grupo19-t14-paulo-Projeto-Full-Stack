  "schemes": ["http"],
  "paths": {
    "/users": {
      "post": {
        "description": "Criação de usuário",
        "tags": ["Users"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "user",
                    "minLength": 2,
                    "maxLength": 127,
                    "required": true
                  },
                  "email": {
                    "type": "string",
                    "example": "user@mail.com",
                    "format": "email",
                    "required": true,
                    "maxLength": 127
                  },
                  "cpf": {
                    "type": "string",
                    "example": "00000000000",
                    "required": true,
                    "maxLength": 11
                  },
                  "phone": {
                    "type": "string",
                    "example": "5541987654321",
                    "required": true,
                    "maxLength": 13
                  },
                  "birthDate": {
                    "type": "date",
                    "example": "2000-01-01",
                    "required": true
                  },
                  "description": {
                    "type": "string",
                    "example": "Descrição teste",
                    "optional": true,
                    "nullable": true
                  },
                  "password": {
                    "type": "string",
                    "example": "12345678",
                    "required": true,
                    "minLength": 2,
                    "maxLength": 127
                  },
                  "type": {
                    "type": "string",
                    "enum": ["Cliente", "Vendedor"],
                    "example": "Cliente",
                    "required": true
                  },
                  "street": {
                    "type": "string",
                    "example": "Rua sem saida",
                    "required": true,
                    "maxLength": 127
                  },
                  "number": {
                    "type": "string",
                    "example": "000",
                    "required": true,
                    "maxLength": 20
                  },
                  "city": {
                    "type": "string",
                    "example": "Cabo Frio",
                    "required": true,
                    "maxLength": 127
                  },
                  "state": {
                    "type": "string",
                    "example": "Rio de Janeiro",
                    "required": true,
                    "maxLength": 127
                  },
                  "cep": {
                    "type": "string",
                    "example": "00000000",
                    "required": true,
                    "maxLength": 8
                  },
                  "complement": {
                    "type": "string",
                    "example": "000",
                    "required": false,
                    "maxLength": 50
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "uuid",
                      "example": "19a6-44c8-8072-1b60b1f7e68d"
                    },
                    "name": {
                      "type": "string",
                      "example": "user"
                    },
                    "email": {
                      "type": "string",
                      "example": "user@mail.com"
                    },
                    "cpf": {
                      "type": "string",
                      "example": "00000000000"
                    },
                    "phone": {
                      "type": "string",
                      "example": "5541987654321"
                    },
                    "birthDate": {
                      "type": "string",
                      "example": "2000-01-01"
                    },
                    "description": {
                      "type": "string",
                      "example": "Descrição teste"
                    },
                    "password": {
                      "type": "string",
                      "example": "12345678"
                    },
                    "type": {
                      "type": "string",
                      "example": "Cliente"
                    },
                    "street": {
                      "type": "string",
                      "example": "Rua sem saida"
                    },
                    "number": {
                      "type": "string",
                      "example": "000"
                    },
                    "city": {
                      "type": "string",
                      "example": "Cabo Frio"
                    },
                    "state": {
                      "type": "string",
                      "example": "Rio de Janeiro"
                    },
                    "cep": {
                      "type": "string",
                      "example": "00000000"
                    },
                    "complement": {
                      "type": "string",
                      "example": "000"
                    },
                    "reset_token": {
                      "type": null,
                      "example": null
                    }
                  }
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Mensagem de erro",
                      "example": "Email already exists"
                    }
                  }
                }
              }
            }
          }
        }
      },
